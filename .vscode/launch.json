{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run proyectofinal-rrhh",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/proyectofinal-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/proyectofinalrrhh/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/rrhh",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/proyectofinal/rrhh\"}"
      },
      "envFile": "${workspaceFolder}/rrhh/.env1",
      "preLaunchTask": "Build proyectofinal"
    }
  ]
}